   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "journals_of_interest = ['Structural and Multidisciplinary Optimization', \n",
    "                        'Computer Methods in Applied Mechanics and Engineering',\n",
    "                        'Water Resources Research', \n",
    "                        'Environmental Modelling & Software', \n",
    "                        'Journal of Hydrology']\n",
    "\n",
    "journals_of_interest = list(map(str.upper, journals_of_interest))\n",
    "pubs_for_journals = wosis.find_pubs_by_journal(RC, journals_of_interest)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Interested in seeing the citations for papers in the identified top journals"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Get citations for each paper\n",
    "paper_citations_per_journal = {}\n",
    "for pubs in pubs_for_journals:\n",
    "    fn = '{}/journal_pub_citations_{}.csv'.format(TMP_DIR, pubs)\n",
    "    paper_citations_per_journal[pubs] = wosis.get_num_citations(pubs_for_journals[pubs], \n",
    "                                                                    WOS_CONFIG,\n",
    "                                                                    cache_as=fn)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "cols_of_interest = ['AU', 'year', 'title', 'journal', 'citations', 'DOI link']\n",
    "collection = pd.DataFrame(columns=cols_of_interest)\n",
    "for journal, df in paper_citations_per_journal.items():\n",
    "    df = wosis.link_to_pub(df)\n",
    "    df['journal'] = df['SO']\n",
    "    df['DOI link'] = df['DOI link'].str.replace('envost', 'envsoft')  # fix incorrect DOI link\n",
    "    df = df.sort_values('citations', ascending=False)\n",
    "    collection = collection.append(df[cols_of_interest], ignore_index=True)\n",
    "    \n",
    "collection.AU = collection.AU.str.replace(\"|\", \" | \")"
   ]
  },
